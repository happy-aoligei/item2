                                <li class="paginate_button" aria-controls="sample_1" tabindex="0" id="sample_1_next"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex+1, pageIndex3 = Model.WarehouseDetails.CurrentPageIndex, pageIndex4 = Model.ScrapDetails.CurrentPageIndex })"><i class="fa fa-angle-right"></i></a></li>
                            }
                        </ul>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
<div class="portlet box red">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-list"></i> 报废记录
        </div>
        <div class="tools">
            <a href="javascript:;" class="collapse" data-original-title="" title=""></a>
        </div>
    </div>
    <div class="portlet-body">

        <table class="table table-striped table-bordered table-hover dataTable no-footer" role="grid">
            <thead>
                <tr>
                    <th>
                        夹具代码
                    </th>
                    <th>
                        夹具系列号
                    </th>
                    <th>
                        申请日期
                    </th>
                    <th>
                        使用寿命
                    </th>
                    <th>
                        报废原因
                    </th>
                    <th>
                        审核状态
                    </th>
                    <th>
                        申请人
                    </th>
                    <th>
                        初审人
                    </th>
                    <th>
                        终审人
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var m in Model.ScrapDetails)
                {
                    <tr>
                        <td>
                            @m.Code
                            @if (m.ScrapDate.GetValueOrDefault().Date == DateTime.Now.Date)
                            {
                                <span class="badge badge-success badge-roundless">New</span>
                            }
                        </td>
                        <td>
                            @m.SeqID
                        </td>
                        <td>
                            @m.ScrapDate
                        </td>
                        <td>
                            @m.Servicelife
                        </td>
                        <td>
                            @m.Reason
                        </td>
                        <td>
                            @if (m.AuditState == "未审核")
                            {
                                <label class="label label-sm label-warning">未审核</label>
                            }
                            else
                            {
                                if (m.AuditState.Contains("初审"))
                                {
                                    <span>初审</span>
                                }
                                else
                                {
                                    <span>终审</span>
                                }
                                if (m.AuditState.Contains("不通过"))
                                {
                                    <label class="label label-sm label-danger">未通过</label>
                                }
                                else
                                {
                                    <label class="label label-sm label-success">已通过</label>
                                }
                            }
                        </td>
                        <td>
                            @m.ApplicantID
                        </td>
                        <td>
                            @m.Auditor1
                        </td>
                        <td>
                            @m.Auditor2
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        <div class="row">
            <div class="col-md-5 col-sm-12">
                <div class="dataTables_info" role="status" aria-live="polite">
                    Total : @Model.ScrapDetails.TotalItemCount entries
                </div>
            </div>
            @if (Model.ScrapDetails.TotalPageCount > 1)
            {
                <div class="col-md-7 col-sm-12">
                    <div class="dataTables_paginate paging_simple_numbers">
                        <ul class="pagination">
                            @if (Model.ScrapDetails.CurrentPageIndex == 1)
                            {
                                <li class="paginate_button previous disabled" aria-controls="sample_1" tabindex="0"><a href="#"><i class="fa fa-angle-left"></i></a></li>
                            }
                            else
                            {
                                <li class="paginate_button previous" aria-controls="sample_1" tabindex="0" id="sample_1_previous"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 = Model.WarehouseDetails.CurrentPageIndex, pageIndex4 = Model.ScrapDetails.CurrentPageIndex-1 })"><i class="fa fa-angle-left"></i></a></li>
                            }
                            @if (Model.ScrapDetails.TotalPageCount <= 5)
                            {
                                for (int i = 1; i <= Model.ScrapDetails.TotalPageCount; ++i)
                                {
                                    if (i == Model.ScrapDetails.CurrentPageIndex)
                                    {
                                        <li class="paginate_button active" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 =  Model.WarehouseDetails.CurrentPageIndex, pageIndex4 = i})">@i</a></li>
                                    }
                                    else
                                    {
                                        <li class="paginate_button" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 =  Model.WarehouseDetails.CurrentPageIndex, pageIndex4 = i})">@i</a></li>
                                    }
                                }
                            }
                            else
                            {
                                if (Model.ScrapDetails.TotalPageCount - 5 >= Model.ScrapDetails.CurrentPageIndex)
                                {
                                    for (int i = Model.ScrapDetails.CurrentPageIndex; i < Model.ScrapDetails.CurrentPageIndex + 4; ++i)
                                    {
                                        if (i == Model.ScrapDetails.CurrentPageIndex)
                                        {
                                            <li class="paginate_button active" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 =  Model.WarehouseDetails.CurrentPageIndex, pageIndex4 = i})">@i</a></li>
                                        }
                                        else
                                        {
                                            <li class="paginate_button" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 =  Model.WarehouseDetails.CurrentPageIndex, pageIndex4 = i})">@i</a></li>
                                        }
                                    }
                                    <li class="paginate_button disabled" aria-controls="sample_1" tabindex="0"><a href="">...</a></li>
                                }
                                else
                                {
                                    for (int i = Model.ScrapDetails.TotalPageCount - 4; i <= Model.ScrapDetails.TotalPageCount; ++i)
                                    {
                                        if (i == Model.ScrapDetails.CurrentPageIndex)
                                        {
                                            <li class="paginate_button active" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 =  Model.WarehouseDetails.CurrentPageIndex, pageIndex4 = i})">@i</a></li>
                                        }
                                        else
                                        {
                                            <li class="paginate_button" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 =  Model.WarehouseDetails.CurrentPageIndex, pageIndex4 = i})">@i</a></li>
                                        }
                                    }
                                }
                            }
                            @if (Model.ScrapDetails.CurrentPageIndex == Model.ScrapDetails.TotalPageCount)
                            {
                                <li class="paginate_button disabled" aria-controls="sample_1" tabindex="0" id="sample_1_next"><a href="#"><i class="fa fa-angle-right"></i></a></li>
                            }
                            else
                            {
                                <li class="paginate_button" aria-controls="sample_1" tabindex="0" id="sample_1_next"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 = Model.WarehouseDetails.CurrentPageIndex, pageIndex4 = Model.ScrapDetails.CurrentPageIndex+1 })"><i class="fa fa-angle-right"></i></a></li>
                            }
                        </ul>
                    </div>
                </div>
            }
        </div>
    </div>
</div>

