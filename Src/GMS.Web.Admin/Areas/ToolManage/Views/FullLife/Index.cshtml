@using GMS.Framework.Contract
@using GMS.Framework.Utility
@using GMS.Framework.Web.Controls
@using GMS.ToolManage.Contract
@using GMS.Web.Admin.Common
@model FullLifeModel
@{
    var permissions = AdminUserContext.Current.LoginInfo.BusinessPermissionList.Select(p => p.ToString());
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section PageSpecificStyleSheetIncludes{
    <link rel="stylesheet" type="text/css" href="/assets/global/plugins/select2/select2.css" />
    <link rel="stylesheet" type="text/css" href="/assets/global/plugins/datatables/extensions/Scroller/css/dataTables.scroller.min.css" />
    <link rel="stylesheet" type="text/css" href="/assets/global/plugins/datatables/extensions/ColReorder/css/dataTables.colReorder.min.css" />
    <link rel="stylesheet" type="text/css" href="/assets/global/plugins/datatables/plugins/bootstrap/dataTables.bootstrap.css" />
}

<div class="portlet box blue">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-list"></i> 详细信息
        </div>
        <div class="tools">
            <a href="javascript:;" class="collapse" data-original-title="" title=""></a>
        </div>
    </div>
    <div class="portlet-body">
        <table class="table table-striped table-bordered table-hover dataTable no-footer">
            <thead>
                <tr>
                    <th>
                        夹具代码
                    </th>
                    <th>
                        夹具系列号
                    </th>
                    <th>
                        模块
                    </th>
                    <th>
                        夹具料号
                    </th>
                    <th>
                        采购单据号
                    </th>
                    <th>
                        入库日期
                    </th>
                    <th>
                        存放库位
                    </th>
                    <th>
                        状态
                    </th>
                    <th>
                        已使用次数
                    </th>
                    <th>
                        故障概率
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        @Model.Details.Code
                    </td>
                    <td>
                        @Model.Details.SeqID
                    </td>
                    <td>
                        @Model.Details.Model
                    </td>
                    <td>
                        @Model.Details.PartNo
                    </td>
                    <td>
                        @Model.Details.BillNo
                    </td>
                    <td>
                        @Model.Details.RegDate.ToString("yyyy-MM-dd HH-mm-ss")
                    </td>
                    <td>
                        @Model.Details.Location-@Model.Details.Bin
                    </td>
                    <td>
                        @Model.Details.State
                    </td>
                    <td>
                        @Model.Details.UsedCount
                    </td>
                    <td>
                        @Model.Details.ErrProbability
                    </td>
                </tr>
            </tbody>
        </table>
        <div class="row" style="text-align:center">
            <a href="@Model.Details.Photo" title="查看大图" target="_blank"><img src="@Model.Details.Photo" alt="无法正常显示" style="width:400px" /></a>
        </div>
    </div>
</div>
<div class="portlet box green">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-list"></i> 采购入库记录
        </div>
        <div class="tools">
            <a href="javascript:;" class="collapse" data-original-title="" title=""></a>
        </div>
    </div>
    <div class="portlet-body">
        <table class="table table-striped table-bordered table-hover dataTable no-footer" role="grid">
            <thead>
                <tr>
                    <th>夹具编号</th>
                    <th>夹具序列号</th>
                    <th>接收编号</th>
                    <th>责任人的职工编号</th>
                    <th>种类</th>
                    <th>模块</th>
                    <th>夹具的接受日期</th>
                    <th>申请人</th>
                    <th>入库初审人</th>
                    <th>入库终审人</th>
                    <th>库位</th>
                    <th>审核状态</th>
                    <th>初审备注</th>
                    <th>终审备注</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var m in Model.WarehouseDetails)
                {
                    <tr>
                        <td>
                            @m.Code
                            @if (m.CreateTime.Date == DateTime.Now.Date)
                            {
                                <span class="badge badge-success badge-roundless">New</span>
                            }
                        </td>
                        <td>
                            @m.SeqID
                        </td>
                        <td>
                            @m.buyoff
                        </td>
                        <td>
                            @m.Owner
                        </td>
                        <td>
                            @m.Family
                        </td>
                        <td>
                            @m.Model
                        </td>
                        <td>
                            @m.jbuyoff
                        </td>
                        <td>
                            @m.Operator2
                        </td>
                        <td>
                            @m.Supervisor
                        </td>
                        <td>
                            @m.Manager
                        </td>
                        <td>
                            @m.Location-@m.bin
                        </td>
                        <td>
                            @if (m.AuditState == "未审核")
                            {
                                <label class="label label-sm label-warning">未审核</label>
                            }
                            else
                            {
                                if (m.AuditState.Contains("初审"))
                                {
                                    <span>初审</span>
                                }
                                else
                                {
                                    <span>终审</span>
                                }
                                if (m.AuditState.Contains("不通过"))
                                {
                                    <label class="label label-sm label-danger">未通过</label>
                                }
                                else
                                {
                                    <label class="label label-sm label-success">已通过</label>
                                }
                            }
                        </td>
                        <td>
                            @m.Remark1
                        </td>
                        <td>
                            @m.Remark2
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        <div class="row">
            <div class="col-md-5 col-sm-12">
                <div class="dataTables_info" role="status" aria-live="polite">
                    Total : @Model.WarehouseDetails.TotalItemCount entries
                </div>
            </div>
            @if (Model.WarehouseDetails.TotalPageCount > 1)
            {
                <div class="col-md-7 col-sm-12">
                    <div class="dataTables_paginate paging_simple_numbers">
                        <ul class="pagination">
                            @if (Model.WarehouseDetails.CurrentPageIndex == 1)
                            {
                                <li class="paginate_button previous disabled" aria-controls="sample_1" tabindex="0"><a href="#"><i class="fa fa-angle-left"></i></a></li>
                            }
                            else
                            {
                                <li class="paginate_button previous" aria-controls="sample_1" tabindex="0" id="sample_1_previous"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 = Model.WarehouseDetails.CurrentPageIndex-1, pageIndex4 = Model.ScrapDetails.CurrentPageIndex })"><i class="fa fa-angle-left"></i></a></li>
                            }
                            @if (Model.WarehouseDetails.TotalPageCount <= 5)
                            {
                                for (int i = 1; i <= Model.WarehouseDetails.TotalPageCount; ++i)
                                {
                                    if (i == Model.WarehouseDetails.CurrentPageIndex)
                                    {
                                        <li class="paginate_button active" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 = i, pageIndex4 = Model.ScrapDetails.CurrentPageIndex })">@i</a></li>
                                    }
                                    else
                                    {
                                        <li class="paginate_button" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 = i, pageIndex4 = Model.ScrapDetails.CurrentPageIndex })">@i</a></li>
                                    }
                                }
                            }
                            else
                            {
                                if (Model.WarehouseDetails.TotalPageCount - 5 >= Model.WarehouseDetails.CurrentPageIndex)
                                {
                                    for (int i = Model.WarehouseDetails.CurrentPageIndex; i < Model.WarehouseDetails.CurrentPageIndex + 4; ++i)
                                    {
                                        if (i == Model.WarehouseDetails.CurrentPageIndex)
                                        {
                                            <li class="paginate_button active" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 = i, pageIndex4 = Model.ScrapDetails.CurrentPageIndex })">@i</a></li>
                                        }
                                        else
                                        {
                                            <li class="paginate_button" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 = i, pageIndex4 = Model.ScrapDetails.CurrentPageIndex })">@i</a></li>
                                        }
                                    }
                                    <li class="paginate_button disabled" aria-controls="sample_1" tabindex="0"><a href="">...</a></li>
                                }
                                else
                                {
                                    for (int i = Model.WarehouseDetails.TotalPageCount - 4; i <= Model.WarehouseDetails.TotalPageCount; ++i)
                                    {
                                        if (i == Model.WarehouseDetails.CurrentPageIndex)
                                        {
                                            <li class="paginate_button active" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 = i, pageIndex4 = Model.ScrapDetails.CurrentPageIndex })">@i</a></li>
                                        }
                                        else
                                        {
                                            <li class="paginate_button" aria-controls="sample_1" tabindex="0"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 = i, pageIndex4 = Model.ScrapDetails.CurrentPageIndex })">@i</a></li>
                                        }
                                    }
                                }
                            }
                            @if (Model.WarehouseDetails.CurrentPageIndex == Model.WarehouseDetails.TotalPageCount)
                            {
                                <li class="paginate_button disabled" aria-controls="sample_1" tabindex="0" id="sample_1_next"><a href="#"><i class="fa fa-angle-right"></i></a></li>
                            }
                            else
                            {
                                <li class="paginate_button" aria-controls="sample_1" tabindex="0" id="sample_1_next"><a href="@Url.Action("Index", new { pageIndex1 = Model.InDetails.CurrentPageIndex, pageIndex2 = Model.OutDetails.CurrentPageIndex, pageIndex3 = Model.WarehouseDetails.CurrentPageIndex+1, pageIndex4 = Model.ScrapDetails.CurrentPageIndex })"><i class="fa fa-angle-right"></i></a></li>
                            }
                        </ul>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
